{
  "name": "meteocontrol/vcom-api-client",
  "description": "HTTP Client for meteocontrol's VCOM API - The VCOM API enables you to directly access your data on the meteocontrol platform.",
  "license": "MIT",
  "keywords": [
    "meteocontrol",
    "vcom",
    "safersun",
    "api",
    "solar",
    "monitoring",
    "client",
    "rest",
    "http"
  ],
  "authors": [
    {
      "name": "meteocontrol GmbH"
    }
  ],
  "require": {
    "php": "^7.4||^8.2",
    "guzzlehttp/guzzle": "^7.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.7",
    "squizlabs/php_codesniffer": "^3.6.1"
  },
  "autoload": {
    "files": [
      "src/Model.php"
    ],
    "psr-4": {
      "meteocontrol\\client\\vcomapi\\": "src",
      "meteocontrol\\vcomapi\\model\\": "src/model",
      "meteocontrol\\client\\vcomapi\\tests\\unit\\": "tests/unit/lib"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
